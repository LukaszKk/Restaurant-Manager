<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #3562e8;" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.RegisterController">
   <children>
      <Label fx:id="labelRegister" layoutX="34.0" layoutY="38.0" prefHeight="44.0" prefWidth="156.0" text="Register">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <TextField fx:id="userName" layoutX="34.0" layoutY="113.0" prefHeight="30.0" prefWidth="182.0" promptText="User Name">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="password" layoutX="34.0" layoutY="170.0" prefHeight="30.0" prefWidth="182.0" promptText="Password">
         <font>
            <Font size="14.0" />
         </font>
      </TextField>
      <ChoiceBox fx:id="choiceBox" layoutX="34.0" layoutY="232.0" prefWidth="150.0" />
      <Button fx:id="signUpButton" layoutX="34.0" layoutY="291.0" mnemonicParsing="false" onAction="#signUpAction" text="Sign Up" />
      <Hyperlink fx:id="back" layoutX="14.0" layoutY="14.0" onAction="#backAction" text="Back" textFill="#1b1a1a" />
      <Hyperlink fx:id="exit" layoutX="545.0" layoutY="14.0" onAction="#exitAction" text="Exit" textFill="#1b1a1a" />
      <Label fx:id="loggedAs" layoutX="36.0" layoutY="369.0" text="Logged in" />
   </children>
</AnchorPane>
