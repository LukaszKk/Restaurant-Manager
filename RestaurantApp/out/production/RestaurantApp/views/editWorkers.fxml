      <Label fx:id="userName" layoutX="251.0" layoutY="118.0" prefHeight="20.0" prefWidth="335.0" text="User Name">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="password" layoutX="251.0" layoutY="175.0" prefHeight="20.0" prefWidth="335.0" text="Password">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="position" layoutX="251.0" layoutY="235.0" prefHeight="20.0" prefWidth="335.0" text="Position">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
