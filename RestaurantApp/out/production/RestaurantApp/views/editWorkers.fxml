<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #3562e8;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.EditWorkersController">
    <children>
        <Label fx:id="labelRegister" layoutX="34.0" layoutY="38.0" prefHeight="44.0" prefWidth="156.0" text="Edit">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <TextField fx:id="newUserName" layoutX="34.0" layoutY="113.0" prefHeight="30.0" prefWidth="182.0" promptText="New User Name">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <TextField fx:id="newPassword" layoutX="34.0" layoutY="170.0" prefHeight="30.0" prefWidth="182.0" promptText="New Password">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <ChoiceBox fx:id="choiceBox" layoutX="34.0" layoutY="232.0" prefWidth="150.0" />
        <Button fx:id="submitButton" layoutX="34.0" layoutY="291.0" mnemonicParsing="false" onAction="#submitAction" text="Submit" />
        <Hyperlink fx:id="back" layoutX="14.0" layoutY="14.0" onAction="#backAction" text="Back" textFill="#1b1a1a" />
        <Hyperlink fx:id="logOut" layoutX="537.0" layoutY="14.0" onAction="#logOutAction" text="Log out" textFill="#1b1a1a" />
