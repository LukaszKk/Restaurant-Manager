<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="396.0" prefWidth="585.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.LoginController">
   <children>
      <Label fx:id="loginLabel" layoutX="262.0" layoutY="41.0" prefHeight="90.0" prefWidth="77.0" text="Login">
         <font>
            <Font size="31.0" />
         </font>
      </Label>
      <TextField fx:id="loginField" layoutX="226.0" layoutY="131.0" promptText="User name" />
      <TextField fx:id="passwordField" layoutX="226.0" layoutY="175.0" promptText="Password" />
      <Button fx:id="signButton" layoutX="262.0" layoutY="238.0" mnemonicParsing="false" onAction="#signButtonAction" prefHeight="25.0" prefWidth="77.0" text="Sign in" />
   </children>
</Pane>
