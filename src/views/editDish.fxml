<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #151928;" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.manager.EditDishController">
    <children>
        <Label fx:id="labelRegister" layoutX="34.0" layoutY="38.0" prefHeight="44.0" prefWidth="156.0" text="Edit">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <TextField fx:id="newDishName" layoutX="34.0" layoutY="113.0" prefHeight="30.0" prefWidth="182.0" promptText="New Dish Name">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <TextField fx:id="newPrice" layoutX="34.0" layoutY="170.0" prefHeight="30.0" prefWidth="182.0" promptText="New Price">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <ChoiceBox fx:id="choiceBox" layoutX="34.0" layoutY="232.0" prefWidth="150.0" />
        <Button fx:id="submitButton" layoutX="34.0" layoutY="291.0" mnemonicParsing="false" onAction="#submitAction" text="Submit" />
        <Button fx:id="deleteButton" layoutX="112.0" layoutY="291.0" mnemonicParsing="false" onAction="#deleteAction" text="Delete" />
        <Hyperlink fx:id="back" layoutX="14.0" layoutY="14.0" onAction="#backAction" text="Back" textFill="#1b1a1a" />
        <Hyperlink fx:id="logOut" layoutX="537.0" layoutY="14.0" onAction="#logOutAction" text="Log out" textFill="#646464" />
        <Label fx:id="loggedAs" layoutX="36.0" layoutY="369.0" text="Logged in" />
        <Label fx:id="dishName" layoutX="251.0" layoutY="118.0" prefHeight="20.0" prefWidth="335.0" text="Dish Name">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="price" layoutX="251.0" layoutY="175.0" prefHeight="20.0" prefWidth="335.0" text="Price">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="category" layoutX="251.0" layoutY="235.0" prefHeight="20.0" prefWidth="335.0" text="Category">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
    </children>
</AnchorPane>
