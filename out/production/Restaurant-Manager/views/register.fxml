<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #151928;" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.account.RegisterController">
    <children>
        <Label fx:id="labelRegister" layoutX="212.0" layoutY="28.0" prefHeight="44.0" prefWidth="156.0" text="Register">
            <font>
                <Font size="25.0" />
            </font>
        </Label>
        <TextField fx:id="userName" layoutX="212.0" layoutY="106.0" prefHeight="30.0" prefWidth="182.0" promptText="Username">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <TextField fx:id="password" layoutX="212.0" layoutY="164.0" prefHeight="30.0" prefWidth="182.0" promptText="Password">
            <font>
                <Font size="14.0" />
            </font>
        </TextField>
        <ChoiceBox fx:id="choiceBox" layoutX="212.0" layoutY="217.0" prefWidth="150.0" />
        <Button fx:id="signUpButton" layoutX="212.0" layoutY="294.0" mnemonicParsing="false" onAction="#signUpAction" text="Sign Up" />
        <Hyperlink fx:id="back" layoutX="14.0" layoutY="14.0" onAction="#backAction" text="Back" textFill="#646464" />
        <Hyperlink fx:id="exit" layoutX="545.0" layoutY="14.0" onAction="#exitAction" text="Exit" textFill="#646464" />
        <Label fx:id="loggedAs" layoutX="433.0" layoutY="18.0" prefHeight="29.0" prefWidth="81.0" style="visibility:hidden" />
    </children>
</AnchorPane>
